        "effect": "^3.x"
    },
    "devDependencies": {
        "@eslint/compat": "^1",
        "@types/chai": "^4",
        "@types/mocha": "^10",
        "@types/node": "^25",
        "@types/ws": "^8.18.1",
        "chai": "^4",
        "eslint": "^9",
        "eslint-config-prettier": "^10",
        "mocha": "^10",
        "shx": "^0.3.3",
        "ts-node": "^10",
        "typescript": "^5"
