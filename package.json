    "version": "0.0.0",
    "author": "The Ghost of Lab Nine",
    "bin": {
        "demeter2": "./bin/run.js"
    },
    "bugs": "https://github.com/bballant/demeter2/issues",
    "dependencies": {
        "@duckdb/node-api": "^1.4.3-r.3",
